    "#| echo: false\n",
    "#| fig-cap: Popular maps show sharp regional boundaries for soft drink terminology.\n",
    "#| label: fig-soda-pop-map\n",
    "#| code-fold: true\n",
    "\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "from matplotlib.patches import Rectangle\n",
    "import matplotlib.patches as mpatches\n",
    "\n",
    "fig, ax = plt.subplots(figsize=(12, 8))\n",
    "\n",
    "# Create a simplified \"map\" showing regions\n",
    "# Northwest/West (soda) - blue\n",
    "ax.add_patch(Rectangle((0, 5), 3, 3, facecolor='#3498db', alpha=0.6, edgecolor='black', linewidth=2))\n",
    "# Northeast (soda) - blue\n",
    "ax.add_patch(Rectangle((7, 5), 3, 3, facecolor='#3498db', alpha=0.6, edgecolor='black', linewidth=2))\n",
    "# Midwest (pop) - orange\n",
    "ax.add_patch(Rectangle((3, 5), 4, 3, facecolor='#e67e22', alpha=0.6, edgecolor='black', linewidth=2))\n",
    "# South (coke) - red\n",
    "ax.add_patch(Rectangle((2, 0), 6, 5, facecolor='#e74c3c', alpha=0.6, edgecolor='black', linewidth=2))\n",
    "# West/Southwest (mixed)\n",
    "ax.add_patch(Rectangle((0, 0), 2, 5, facecolor='#9b59b6', alpha=0.6, edgecolor='black', linewidth=2))\n",
    "\n",
    "# Add labels\n",
    "ax.text(1.5, 6.5, 'SODA', fontsize=20, fontweight='bold', ha='center', va='center')\n",
    "ax.text(8.5, 6.5, 'SODA', fontsize=20, fontweight='bold', ha='center', va='center')\n",
    "ax.text(5, 6.5, 'POP', fontsize=20, fontweight='bold', ha='center', va='center')\n",
    "ax.text(5, 2.5, 'COKE', fontsize=20, fontweight='bold', ha='center', va='center')\n",
    "\n",
    "ax.set_xlim(0, 10)\n",
    "ax.set_ylim(0, 8)\n",
    "ax.set_aspect('equal')\n",
    "ax.axis('off')\n",
    "ax.set_title('The Myth of Sharp Dialect Boundaries', fontsize=18, fontweight='bold', pad=20)\n",
    "\n",
    "# Add legend\n",
    "blue_patch = mpatches.Patch(color='#3498db', alpha=0.6, label='Soda Region')\n",
    "orange_patch = mpatches.Patch(color='#e67e22', alpha=0.6, label='Pop Region')\n",
    "red_patch = mpatches.Patch(color='#e74c3c', alpha=0.6, label='Coke Region')\n",
    "ax.legend(handles=[blue_patch, orange_patch, red_patch], loc='upper left', fontsize=12)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
